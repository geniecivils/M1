<script>
document.addEventListener('contextmenu', function(event) {
    event.preventDefault();
});

document.addEventListener('keydown', function(event) {
    if (event.key === 'F12' || 
        (event.ctrlKey && event.shiftKey && (event.key === 'I' || event.key === 'J')) ||
        (event.ctrlKey && event.key === 'U')) {
        event.preventDefault();
    }
});

const correctPassword = "20242025";

function checkPassword() {
    const userPassword = document.getElementById("password").value;
    if (userPassword === correctPassword) {
        const timestamp = new Date().getTime();
        localStorage.setItem("isAuthenticated", "true");
        localStorage.setItem("authTimestamp", timestamp); // Save the timestamp
        setPageState("selectionContainer");
        goToPage("selectionContainer");
    } else {
        document.getElementById("errorMessage").style.display = "block";
    }
}


// Toggle password visibility
const togglePassword = document.getElementById("togglePassword");
const passwordField = document.getElementById("password");

togglePassword.addEventListener("click", () => {
    // Toggle the type of password field between 'password' and 'text'
    const type = passwordField.type === "password" ? "textpass" : "password";
    passwordField.type = type;
    
    // Change the icon depending on the type
    togglePassword.textContent = type === "password" ? "üëÅÔ∏è" : "‚ö™";

    passwordInput.focus();
});

document.getElementById('togglePassword').addEventListener('click', () => {
      document.getElementById('password').focus();
  });


function goToPage(pageId) {
    const containers = document.querySelectorAll(".container > div");
    containers.forEach((container) => {
        container.style.display = "none";
    });

    document.getElementById(pageId).style.display = "block";
    localStorage.setItem("currentPage", pageId);
}

function returnToSelection() {
    setPageState("selectionContainer");
    goToPage("selectionContainer");
}

function setPageState(pageId) {
    localStorage.setItem("currentPage", pageId);
}

function togglePdfList(id) {
    const allPdfLists = document.querySelectorAll(".pdf-list");
    const allIcons = document.querySelectorAll(".model-title span");

    allPdfLists.forEach((list) => {
        if (list.id !== id) {
            list.style.display = "none";
        }
    });

    allIcons.forEach((icon) => {
        icon.textContent = "‚ñº";
    });

    const pdfList = document.getElementById(id);
    const titleIcon = pdfList.previousElementSibling.querySelector("span");
    if (pdfList.style.display === "none" || pdfList.style.display === "") {
        pdfList.style.display = "flex";
        pdfList.style.flexWrap = "wrap";
        pdfList.style.justifyContent = "center";
        titleIcon.textContent = "‚ñ≤";
        localStorage.setItem("openedPdfList", id);  
    } else {
        pdfList.style.display = "none";
        titleIcon.textContent = "‚ñº";
        localStorage.removeItem("openedPdfList");  
    }
}

function restorePageState() {
    const isAuthenticated = localStorage.getItem("isAuthenticated");
    const authTimestamp = localStorage.getItem("authTimestamp");
    const currentTime = new Date().getTime();

    // Check if the password is expired
    if (isAuthenticated === "true" && authTimestamp && (currentTime - authTimestamp) < 86400000) { // 1 day = 86400000ms
        const currentPage = localStorage.getItem("currentPage");
        if (currentPage) {
            goToPage(currentPage);
        } else {
            goToPage("selectionContainer");
        }

        const openedPdfListId = localStorage.getItem("openedPdfList");
        if (openedPdfListId) {
            togglePdfList(openedPdfListId);
        }
    } else {
        localStorage.removeItem("isAuthenticated");
        localStorage.removeItem("authTimestamp");
        goToPage("loginContainer");
    }
}

document.getElementById("password").addEventListener("keydown", function (event) {
    if (event.key === "Enter") {
        checkPassword();
    }
});

window.onload = restorePageState;
</script>

